{
  "name": "imageine",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "node --env-file=.env --import=tsx --watch ./src/index.ts",
    "start": "node --import=tsx ./src/index.ts",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "format": "biome format . --log-level='info' --log-kind='pretty' --error-on-warnings --diagnostic-level='info' --write",
    "lint": "biome lint . --error-on-warnings --colors='force' --log-level='info' --log-kind='pretty' --diagnostic-level='info' --apply-unsafe"
  },
  "dependencies": {
    "h3": "^1.13.0",
    "ipx": "^3.0.1",
    "listhen": "^1.9.0",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.10.2",
    "consola": "^3.2.3",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
